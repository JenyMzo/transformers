{
  "name": "transformers-site",
  "version": "2.0.0",
  "description": "Transformers Website powered by NuxtJS",
  "author": "Transformers",
  "private": false,
  "engines": {
    "node": ">=10.13.0"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt generate",
    "contributors:add": "npx all-contributors add",
    "contributors:generate": "npx all-contributors generate"
  },
  "dependencies": {
    "@nuxtjs/font-awesome": "^1.0.3",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/pwa": "^3.3.5",
    "animate.css": "^3.7.0",
    "bootstrap": "^3.3.7",
    "bootstrap-vue": "^2.0.0",
    "cross-env": "^5.2.0",
    "jsonp": "^0.2.1",
    "nuxt": "^2.0.0",
    "vue-js-modal": "^1.3.26"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.1",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.11.0",
    "push-dir": "^0.4.1",
    "sass-loader": "^7.1.0"
  }
}
